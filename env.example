# Database URLs - These will be provided by Vercel when you create a Postgres database
POSTGRES_PRISMA_URL="postgres://username:password@hostname:port/database?schema=public&pgbouncer=true&connect_timeout=15"
POSTGRES_URL_NON_POOLING="postgres://username:password@hostname:port/database?schema=public"

# OnchainKit API Key - Get this from Coinbase Developer Platform
NEXT_PUBLIC_ONCHAINKIT_API_KEY="your_onchainkit_api_key_here"

# Base Verify Configuration
NEXT_PUBLIC_BASE_VERIFY_URL="https://your-base-verify.ngrok-free.app"
NEXT_PUBLIC_BASE_VERIFY_MINI_APP_URL="cbwallet://miniapp?url=https://miniapp.verify.base.dev"
BASE_VERIFY_SECRET_KEY="your_base_verify_secret_key_here"
BASE_VERIFY_PUBLISHER_KEY="your_base_verify_publisher_key_here"
NEXT_PUBLIC_APP_URL="cbwallet://miniapp?url=https://your-app.vercel.app"
